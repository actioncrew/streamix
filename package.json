{
  "name": "streamix",
  "version": "2.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "jasmine": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/jasmine/bin/jasmine.js --config=spec/support/jasmine.json --verbose",
    "jasmine:coverage": "c8 --reporter=html --reporter=text node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/jasmine/bin/jasmine.js --config=spec/support/jasmine.json --verbose",
    "minify": "node terser.mjs",
    "bundle": "dts-bundle-generator --config ./bundle.config.cjs && npm run minify",
    "clean": "eslint --config eslint.config.js --fix"
  },
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^19.0.2",
    "@angular-eslint/eslint-plugin-template": "^19.0.2",
    "@angular/cli": "^18.0.1",
    "@angular/compiler-cli": "^18.0.0",
    "@types/jasmine": "^5.1.7",
    "@types/node": "^22.13.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "axios": "^1.8.1",
    "c8": "^10.1.3",
    "cors": "^2.8.5",
    "jasmine": "^5.6.0",
    "dts-bundle-generator": "^9.5.1",
    "ejs": "^3.1.10",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "express": "^4.21.2",
    "ng-packagr": "^18.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.2"
  },
  "type": "module",
  "sideEffects": false
}
