{
  "name": "streamix",
  "version": "2.0.27",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "jasmine": "npx ts-test-runner --headless",
    "jasmine:coverage": "npx ts-test-runner --headless --coverage",
    "minify": "node terser.mjs && npx dts-bundler",
    "clean": "eslint --config eslint.config.js --fix",
    "docs:generate": "typedoc --plugin typedoc-plugin-markdown --out docs/api --hideBreadcrumbs --hidePageHeader",
    "docs:dev": "npm run docs:generate && vitepress dev docs",
    "docs:build": "npm run docs:generate && vitepress build docs"
  },
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@actioncrew/dts-bundler": "^1.0.11",
    "@actioncrew/ts-test-runner": "^1.0.12",
    "@angular-devkit/build-angular": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^19.0.2",
    "@angular-eslint/eslint-plugin-template": "^19.0.2",
    "@angular/cli": "^18.0.1",
    "@angular/compiler-cli": "^18.0.0",
    "@types/jasmine": "^5.1.8",
    "@types/markdown-it": "^13.0.9",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "axios": "^1.11.0",
    "c8": "^10.1.3",
    "codecov": "^3.8.3",
    "cors": "^2.8.5",
    "dts-bundle-generator": "^9.5.1",
    "ejs": "^3.1.10",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "express": "^4.21.2",
    "jasmine": "^5.9.0",
    "ng-packagr": "^18.0.0",
    "rollup": "^4.52.4",
    "strip-json-comments": "^5.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.10",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "~5.4.2",
    "vitepress": "^1.6.4"
  },
  "type": "module",
  "sideEffects": false
}
